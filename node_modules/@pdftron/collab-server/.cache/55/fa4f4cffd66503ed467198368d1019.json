ource":"util/getRequestedFields.ts","name":null,"original":{"line":84,"column":40},"generated":{"line":1,"column":820}}],"sources":{"util/getRequestedFields.ts":"import { DocumentNode, FragmentDefinitionNode, OperationDefinitionNode, parse, SelectionNode } from 'graphql';\n\nexport type DefinitionNodes = {\n  fieldNodes: readonly SelectionNode[];\n  fragments: FragmentDefinitionNode[];\n}\n\n/**\n * \n * @param queryString The original query string from context.req\n * @returns fieldNodes that contain query fields and fragments that contain fragment fields\n */\nconst convertQueryStringToDefinitionNodes= (queryString: string): DefinitionNodes => {\n  const query: DocumentNode = parse(queryString);\n    const operation = query.definitions\n        .find(({ kind }) => kind === 'OperationDefinition') as OperationDefinitionNode;\n    const fragments = query.definitions\n      .filter(({ kind }) => kind === 'FragmentDefinition').map( f => f as FragmentDefinitionNode );\n    return {\n        fieldNodes: operation.selectionSet.selections,\n        fragments,\n    };\n};\n\n\nconst getSelections = (root, flattenedTree) => {\n  if(!root) return;\n  if(root.selectionSet?.selections) {\n    root.selectionSet.selections.forEach( s => getSelections(s, flattenedTree));\n  }\n  if(root.kind === 'Field') flattenedTree[root.name.value] = true;\n}\n\nconst getFlattenedFieldsTree = (resolveInfo): RequestedFields => {\n  const flattenedTree = {};\n  const root = resolveInfo.fieldNodes[0];\n  getSelections(root, flattenedTree);\n  //ignore rootValues\n  delete flattenedTree[root.name.value];\n  resolveInfo.fragments.forEach( f => {\n      getSelections(f, flattenedTree);\n      delete flattenedTree[f.name.value]\n  });\n  //ignore typename\n  delete flattenedTree['__typename'];\n\n  return flattenedTree;\n}\n\nexport type RequestedFields = Record<string, boolean>;\n\n/**\n * \n * @param queryString The original query string from context.req\n * @returns A flattened tree of all queried fields as key and true as value.\n * Example -\n * queryString is \n *  `query user($id: ID!) {\n        user(id: $id) {\n          id\n          email \n          documents {\n            annotations {\n              membership {\n                lastRead\n              }\n            }\n          }\n        }\n    }`\n * The returned tree of fields will look like \n    {\n      id: true,\n      email: true,\n      documents: true,\n      annotations: true,\n      membership: true,\n      lastRead: true\n    }\n */\nexport const getRequestedFields = (queryString: string): RequestedFields => {\n  if(!queryString) return null;\n  const defNodes = convertQueryStringToDefinitionNodes(queryString);\n  const fields = getFlattenedFieldsTree(defNodes);\n  return fields;\n}"},"lineCount":null}},"error":null,"hash":"a01b8f7708a92fa83582f56db3a336db","cacheData":{"env":{}}}