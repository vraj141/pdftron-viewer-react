./shared/log/logger.ts","name":null,"original":{"line":38,"column":15},"generated":{"line":1,"column":1239}}],"sources":{"../../../shared/log/logger.ts":"import winston, { format } from 'winston';\nimport { addConsoleTransport, addTransports } from './transports';\n\nexport enum Tags {\n  STANDARD = 'STANDARD',\n  ENTITY = 'ENTITY',\n  QUERY = 'QUERY',\n  RESPONSE = 'RESPONSE'\n}\n\nexport type Logging = 'error' | 'warn' | 'info' | 'http' | 'verbose' | 'debug' | 'silly' | 'none';\n\nexport const LogLevels = {\n  ERROR: 'error',\n  WARN: 'warn',\n  INFO: 'info',\n  HTTP: 'http',\n  VERBOSE: 'verbose',\n  DEBUG: 'debug',\n  SILLY: 'silly',\n  NONE: 'none'\n} as const;\n\nconst logger = winston.createLogger({\n  format: format.combine(format.timestamp({ format: 'YYYY-MM-DD HH:mm:ss' }))\n});\n\nexport const setupLogger = (logLevel: Logging, logTransports: Array<winston.transport>) => {\n  if (logLevel === LogLevels.NONE) {\n    // disable all logs\n    logger.silent = true;\n  } else {\n    addConsoleTransport(logger, logLevel);\n    logTransports && logTransports.length && addTransports(logger, logTransports);\n  }\n};\n\nexport default logger;\n"},"lineCount":null}},"error":null,"hash":"b7fe369cafd97c513f3c62cb9c8577e0","cacheData":{"env":{}}}